\begin_layout Subsubsection*
Initialization
\end_layout

\begin_layout Standard
For 
\begin_inset Formula $t<=D\_MAX$
\end_inset

:
\end_layout

\begin_layout Standard
——————–
\end_layout

\begin_layout Standard
\begin_inset Formula $\delta_{t}(i)=max\{\delta(i)*,\kappa_{t}(i)\}$
\end_inset


\end_layout

\begin_layout Standard
where a reduced-duration delta is defined as:
\end_layout

\begin_layout Standard
\begin_inset Formula $\delta_{t}(i)*=max_{d}\{\delta_{t-d}(i-1)p_{i}(d)\Pi_{s=t-d+1}^{t}b_{i}(O_{s})\}$
\end_inset


\end_layout

\begin_layout Standard
with 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $d\in(1,min(t-1,max(i))$
\end_inset

 reduces the duration to 
\begin_inset Formula $t$
\end_inset

 when 
\begin_inset Formula $t<max(i)$
\end_inset


\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
